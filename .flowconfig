[ignore]
.*/node_modules/draft-js/*
.*/node_modules/jsonlint/*
.*/node_modules/distraught/src/*
.*/node_modules/google-cloud/*
.*/node_modules/react-leaflet/*
.*node_modules/node-uuid/lib/sha1-browser.js
.*/node_modules/react-draft-wysiwyg/*
.*/node_modules/react-virtualized/*
.*/node_modules/@elm-street-technology/es
client/assets/*/*
tests-e2e/**/*

[include]
./node_modules/react
./node_modules/fbjs/lib
./node_modules/immutable

[lints]
deprecated-utility=off

[libs]
flow/declarations/

[options]
module.system.node.resolve_dirname=.
module.system.node.resolve_dirname=node_modules
module.name_mapper='Constant' -> 'server/lib/constants'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
suppress_comment=\\(.\\|\n\\)*\\$FlowBug


[version]
^0.99.1
