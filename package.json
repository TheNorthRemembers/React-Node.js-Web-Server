{
  "name": "react-nodejs-starter-kit",
  "version": "0.0.1",
  "description": "Quickstart React App on a Node.js Server",
  "author": "Michael Bondi",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "concurrently -k \"yarn run web\" \"yarn run webpack\"",
    "start-once": "concurrently -k \"yarn run web-once\" \"yarn run webpack\"",
    "build": "yarn clean && yarn build-structure && yarn copy-static-files && concurrently \"yarn build-root\" \"yarn build-server\" \"gulp deploy\" \"yarn webpack:prod\"",
    "build-structure": "mkdir -p dist/server/web/public/js/react",
    "webpack": "webpack --mode development --watch --config webpack.config.dev.js",
    "webpack:prod": "NODE_ENV=production webpack --mode production --config webpack.config.prod.js",
    "build-root": "NODE_ENV=production babel index.js web.js -d dist",
    "build-server": "NODE_ENV=production babel server --x \".jsx?\" --ignore server/web/public/js/*,server/web/views/{!json} -d dist/server",
    "copy-static-files": "cp package.json dist/package.json",
    "docker": "make docker-up",
    "flow": "flow status",
    "foreman": "nf --procfile Procfile.dev start",
    "format": "prettier --write \"**/*.{js,jsx,scss}\"",
    "format-check": "prettier -l \"**/*.{js,jsx,scss}\"",
    "lint": "nf run -- eslint --ext .js .",
    "version-major": "./dev-ops/version major",
    "version-minor": "./dev-ops/version minor",
    "version-patch": "./dev-ops/version patch",
    "release-candidate-major": "./dev-ops/rc major",
    "release-candidate-minor": "./dev-ops/rc minor",
    "migrate": "nf run -- mariner migrate up",
    "rollback": "nf run -- mariner migrate down",
    "web": "nf run -- babel-watch -L index.js web",
    "web-as-qa": "nf run -- NODE_PATH=dist NODE_ENV=qa node dist/index.js web",
    "web-as-prod": "nf run -- NODE_PATH=dist NODE_ENV=production node dist/index.js web",
    "web-debug": "nf run -- babel-watch --inspect index.js web"
  },
  "repository": {
    "type": "git",
    "url": "need to fill this out"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-watch": "^2.0.7",
    "bluebird": "3.5.1",
    "classnames": "^2.2.5",
    "style-loader":"^1.0.0",
    "distraught": "^4.5.1",
    "form-data": "^2.3.3",
    "formik": "^1.0.2",
    "glob": "^7.1.2",
    "joi": "9.2.0",
    "lodash": "^4.17.13",
    "mariner": "^1.0.2",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.11",
    "react": "^16.8.0",
    "react-dom": "^16.8.2",
    "recharts": "^1.8.5",
    "@material-ui/core":"^4.5.1",
    "@material-ui/pickers":"^3.2.7",
    "@date-io/date-fns":"^1.3.11",
    "date-fns@next":"^2.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-recharts":"^1.2.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-register": "^6.9.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.99.1",
    "flow-coverage-report": "^0.4.0",
    "foreman": "^3.0.1",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "prettier": "1.11.1",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-pug": "^3.3.0",
    "gulp-replace": "^0.6.1"
  },
  "engines": {
    "node": "^8.*"
  },
  "flow-coverage-report": {
    "includeGlob": [
      "server/**/*.js",
      "server/**/*.jsx"
    ],
    "excludeGlob": [
      "server/web/public/**"
    ],
    "type": [
      "text",
      "html",
      "json"
    ],
    "threshold": 75
  }
}
